{
    "name": "jason/ucsc-comm-genesis-child",
    "description": "Composer for WP",
    "type": "composer-plugin",
    "require": {
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Jason\\UcscCommGenesisChild\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jason Chafin",
            "email": "jasonchafin@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
